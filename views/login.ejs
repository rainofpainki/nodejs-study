<% layout('boilerplate') %>
<div id="header">
    <h2>
  Node.js Study
</h2>
</div>
<div id="container">
    <form action="/login" method="post" id="registerForm">
      <ul class="list-group">
        <li class="list-group-item">
          <input type="text" name="id" class="form-control input-lg" placeholder="아이디">
          <input type="password" name="pwd" class="form-control input-lg" placeholder="비밀번호">
        </li>
      </ul>
      <button type="submit" id="login" class="btn btn-lg btn-primary center-block" autocomplete="off">로그인</button>
    </form>
</div>
